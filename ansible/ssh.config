Host *
    #disable host key checking: avoid asking for the keyprint authenticity
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    #enable hashing known_host file
    HashKnownHosts yes
    #IdentityFile allows to specify exactly which private key I wish to use for authentification
    IdentityFile /home/zmk/tools/ansible/tests/sandbox/sal-ansible-key
Host festive_sutherland
    HostName localhost
    User sal-ansible
    Port 2222
Host intelligent_antonelli
    HostName localhost
    User sal-ansible
    Port 2223
